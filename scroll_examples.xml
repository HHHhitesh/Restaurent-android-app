    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/imghitesh"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/hitesh"
            android:layout_margin="10dp"
            />
        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hitesh Choudhary"
            android:layout_toRightOf="@id/imghitesh"
            android:layout_marginTop="15dp"
            android:textColor="#000000"
            android:textSize="18sp"

            />
        <TextView
            android:id="@+id/gmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="informationsH.webs@gmail.com"
            android:layout_toRightOf="@id/imghitesh"
            android:layout_below="@id/name"
            android:textColor="#000000"
            android:textSize="18sp"
            android:layout_marginTop="20dp"

            />
        <TextView
            android:id="@+id/number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="+91 7748020897"
            android:layout_toRightOf="@id/imghitesh"
            android:layout_below="@id/gmail"
            android:textColor="#000000"
            android:textSize="18sp"
            android:layout_marginTop="20dp"
            />
    </RelativeLayout>
